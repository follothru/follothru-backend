    "chai": "^4.2.0",
    "mocha": "^6.1.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.11",
    "mongoose": "^5.5.14",
    "passport": "^0.4.0",
